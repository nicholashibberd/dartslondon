<p id="notice"><%= notice %></p>

<h1><%= @region.name %></h1>

<table class="table table-striped">
  <tbody>
    <% @pubs.each do |pub| %>
      <tr>
        <td><%= link_to pub.name, pub %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_region_path(@region), class: 'btn btn-default' %>
